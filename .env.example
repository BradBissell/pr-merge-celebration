# GitHub Personal Access Token with repo read permissions
GITHUB_TOKEN=ghp_your_token_here

# Slack Webhook URL (from Incoming Webhooks app)
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL

# Comma-separated list of repositories to check (format: owner/repo)
REPOS_TO_CHECK=octocat/Hello-World,microsoft/vscode

# Number of hours to look back for merged PRs (optional, default: 24)
MERGE_WINDOW=24
